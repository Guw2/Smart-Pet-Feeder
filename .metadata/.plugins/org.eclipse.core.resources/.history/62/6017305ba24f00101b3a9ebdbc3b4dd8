import org.springframework.stereotype.Service;

@Service
public class PetDetectionService {

	private final PetDetectionRepository repo;
	private final PetDetectionService service;
	
	public PetDetectionService(PetDetectionRepository repo, PetDetectionService service) {
		this.repo = repo;
		this.service = service;
	}
	
	public 
	
}
