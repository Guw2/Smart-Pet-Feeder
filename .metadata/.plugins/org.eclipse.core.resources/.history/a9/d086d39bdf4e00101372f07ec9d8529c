package com.lorian.feeder_service;

public class Main {
	
	
	private static String firstUniqueChar(String s) {
		String characters = "";
		
		for(Character c : s.toCharArray()) {
			if(!characters.contains(c + "")) {
				characters += c;
			}else {
				characters = characters.replace(c + "", "");
			}
		}
		
		return characters;
	}
	
	public static void main(String[] args) {
		System.out.println(firstUniqueChar("aabbcddeffgg"));
	}
}
