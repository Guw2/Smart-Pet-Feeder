package com.lorian.feeder_service;

public class Main {
	
	
	private static String firstUniqueChar(String s) {
		int[] characters = new int[256];
		String response = "";
		
		for(char c : s.toCharArray()) {
			characters[c]++;
			if(characters[c] == 1) {
				response += c;
			}else {
				response = response.replace(c + "", "");
			}
		}
		
		return response=="" ? null : response;
		
	}
	
	public static void main(String[] args) {
		System.out.println(firstUniqueChar("aabbfnccddeeffgng"));
	}
}
